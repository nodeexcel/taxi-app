{
  "name": "taxi-app",
  "version": "1.0.0",
  "description": " A small ride sharing app backend written in js and express",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.mjs",
    "lint": "eslint ./**/*.mjs --fix  && npx prettier --write -u .",
    "precommit": "npm run lint && git add .",
    "start": "node ./src/index.mjs",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodeexcel/taxi-app.git"
  },
  "author": "nodeexcel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nodeexcel/taxi-app/issues"
  },
  "homepage": "https://github.com/nodeexcel/taxi-app#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.9.0",
    "sequelize": "^6.28.1",
    "swagger-ui-express": "^4.6.1"
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "sequelize-cli": "^6.6.0"
  }
}
